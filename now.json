{
  "name": "marcradziwill-sl",
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" } }
  ],
  "routes": [
    {
      "src": "api/carbon",
      "dest": "carbon.js",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
